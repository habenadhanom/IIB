

CREATE COMPUTE MODULE functionprocudderMF_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE i INTEGER 7;
		DECLARE j INTEGER 15;
		--DECLARE sum CHARACTER;
		--DECLARE student CHARACTER;
		 
		set OutputRoot.XMLNSC.SU = ADDING(i, j);
		RETURN TRUE;
	END;

	CREATE PROCEDURE ADDING(IN x INTEGER, IN y INTEGER) 
	BEGIN
	    SET Su = 'x + y';
		
	END;
END MODULE;
